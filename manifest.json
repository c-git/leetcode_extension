{
  "description": "Hides the difficulty on leetcode problems",
  "manifest_version": 2,
  "name": "Leetcode-Difficulty",
  "version": "0.1",
  "homepage_url": "https://github.com/c-git/leetcode_extension",
  "icons": {
    "48": "icons/see-no-evil-monkey.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problems/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}